{
  "name": "wwebdev",
  "version": "1.1.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "esbuild": "allfiles=$(ls -1 ./.next/standalone/**/*.js) && esbuild $allfiles --minify --outdir=.next/standalone --platform=node --target=node16 --format=cjs --allow-overwrite",
    "start": "next start",
    "export": "npm run build && next export",
    "postexport": "node scripts/postExport.js",
    "cleandeploy": "aws s3 sync ./out s3://wwebdev-static-website-bucket --delete",
    "deploy": "aws s3 sync ./out s3://wwebdev-static-website-bucket",
    "postcleandeploy": "aws cloudfront create-invalidation --distribution-id=E32J3ET4M6TIBY --paths \"/*\"",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id=E32J3ET4M6TIBY --paths \"/*\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.122",
    "@mui/material": "^5.11.12",
    "@mui/styled-engine-sc": "^5.11.11",
    "@mui/styles": "^5.11.12",
    "@next/bundle-analyzer": "^12.3.1",
    "axios": "^0.21.1",
    "esbuild-wasm": "^0.17.11",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "hamburger-react": "^2.5.0",
    "html-react-parser": "^3.0.4",
    "image-downloader": "^4.0.1",
    "lodash": "^4.17.19",
    "next": "^13.2.3",
    "next-pwa": "^5.6.0",
    "node-fetch": "^2.6.1",
    "react": "^18.2.0",
    "react-color": "^2.18.1",
    "react-dom": "^18.2.0",
    "react-lazy-load": "^4.0.1",
    "react-sizeme": "^3.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "request": "^2.88.2",
    "resize-polyfill": "^1.1.1",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "mjml": "^4.13.0"
  }
}
